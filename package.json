{
  "name": "towerdefense",
  "version": "1.0.0",
  "description": "td game and editor",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node build/build.js TurnBasedWarfare",
    "build:prod": "node build/build.js TurnBasedWarfare --production",
    "build:watch": "node build/build.js TurnBasedWarfare --watch",
    "resources:watch": "node build/watch-resources.js TurnBasedWarfare",
    "dev": "webpack serve --config webpack.dev.config.js",
    "dev:server": "node server_editor.js",
    "game:server": "node projects/TurnBasedWarfare/server_game.js",
    "game:server:dev": "nodemon --watch projects/TurnBasedWarfare/dist/server projects/TurnBasedWarfare/server_game.js",
    "game:dev": "concurrently -n \"BUILD,SERVER,RESOURCES\" -c \"blue,green,magenta\" \"npm run build:watch\" \"npm run game:server:dev\" \"npm run resources:watch\"",
    "game:build-and-start": "npm run build && npm run game:server",
    "start:editor": "node server_editor.js 5000",
    "build:seeders": "PROJECT_NAME=Seeders node build/build.js Seeders",
    "webpack": "webpack --config webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.21.2",
    "jszip": "^3.10.1",
    "multer": "^1.4.5-lts.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "three": "^0.181.0",
    "three-nebula": "^10.0.3"
  },
  "devDependencies": {
    "chokidar": "^4.0.3",
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "codemirror": "^5.65.18",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "nodemon": "^3.1.11",
    "style-loader": "^4.0.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
}
